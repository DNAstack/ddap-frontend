{
  "name": "ddapfrontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --watch --output-path=\"../target/classes/static/\"",
    "build:watch": "nodemon --exec \"ng lint && ng build --output-path=\"../target/classes/static/\"\" --ext ts,html,scss",
    "build:quickwatch": "nodemon --exec \"ng build --output-path=\"../target/classes/static/\"\" --ext ts,html,scss",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "build:prod": "npm run build -- --prod",
    "proto": "./scripts/generate_proto.sh",
    "proto:download": "./scripts/download_proto.sh",
    "postinstall": "npm run proto"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.1",
    "@angular/cdk": "~7.2.0",
    "@angular/common": "~7.2.1",
    "@angular/compiler": "~7.2.1",
    "@angular/core": "~7.2.1",
    "@angular/forms": "~7.2.1",
    "@angular/material": "^7.2.0",
    "@angular/platform-browser": "~7.2.1",
    "@angular/platform-browser-dynamic": "~7.2.1",
    "@angular/router": "~7.2.1",
    "@ngx-loading-bar/core": "^4.1.1",
    "@ngx-loading-bar/http-client": "^4.1.1",
    "ang-jsoneditor": "1.7.5",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "jsoneditor": "^5.27.0",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.set": "^4.3.2",
    "moment": "^2.24.0",
    "ng-pick-datetime": "^7.0.0",
    "ng-pick-datetime-moment": "^1.0.8",
    "ngx-chips": "^2.0.0-beta.0",
    "ngx-clipboard": "^11.1.9",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/cli": "^7.1.4",
    "@angular/compiler-cli": "^7.2.12",
    "@angular/language-service": "~7.2.1",
    "@types/jasmine": "~3.3.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/long": "^4.0.0",
    "@types/node": "~10.12.18",
    "codelyzer": "~4.5.0",
    "husky": "^1.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^8.1.5",
    "nodemon": "^1.18.9",
    "protobufjs": "^6.8.8",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.12.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "~3.2.4"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js,ts,tsx}": [
        "tslint --fix --format stylish",
        "git add"
      ]
    },
    "ignore": [
      "src/app/shared/proto/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
