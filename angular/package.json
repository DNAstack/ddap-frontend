{
  "name": "ddapfrontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --watch --output-path=\"../target/classes/static/\"",
    "build:prod": "ng build --prod",
    "build:staging": "ng build --configuration=staging",
    "build:watch": "nodemon --exec \"ng lint && ng build --output-path=\"../target/classes/static/\"\" --ext ts,html,scss",
    "build:quickwatch": "nodemon --exec \"ng build --output-path=\"../target/classes/static/\"\" --ext ts,html,scss",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "proto": "./scripts/generate_proto.sh",
    "proto:download": "./scripts/download_proto.sh",
    "postinstall": "npm run proto"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.0",
    "@angular/cdk": "~8.0.2",
    "@angular/common": "^8.1.0",
    "@angular/compiler": "~8.1.0",
    "@angular/core": "^8.1.0",
    "@angular/forms": "^8.1.0",
    "@angular/material": "^8.0.2",
    "@angular/platform-browser": "~8.1.0",
    "@angular/platform-browser-dynamic": "~8.1.0",
    "@angular/router": "~8.1.0",
    "@ngx-loading-bar/core": "^4.2.0",
    "@ngx-loading-bar/http-client": "^4.2.0",
    "ajv": "^6.10.1",
    "ang-jsoneditor": "1.8.0",
    "core-js": "^3.1.4",
    "hammerjs": "^2.0.8",
    "jsoneditor": "^6.1.0",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.set": "^4.3.2",
    "moment": "^2.24.0",
    "ngx-chips": "^2.0.2",
    "ngx-clipboard": "^12.2.0",
    "ngx-webstorage-service": "^4.0.1",
    "rxjs": "~6.5.2",
    "rxjs-compat": "^6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.801.0",
    "@angular/cli": "^8.1.0",
    "@angular/compiler-cli": "^8.1.0",
    "@angular/language-service": "~8.1.0",
    "@types/jasmine": "~3.3.13",
    "@types/jasminewd2": "~2.0.6",
    "@types/long": "^4.0.0",
    "@types/node": "~12.6.2",
    "codelyzer": "~5.1.0",
    "husky": "^3.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^9.1.0",
    "nodemon": "^1.19.1",
    "protobufjs": "^6.8.8",
    "protractor": "~5.4.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "~3.4.5"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "tslint --fix --format stylish",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
